# address on the Docker host to expose the storage database port on
ISOL_ENTITY_STORAGEDB_DOCKER_PORT=127.0.0.1:8023
# address on the Docker host to expose the index database ports on
ISOL_ENTITY_INDEXDB_ACI_DOCKER_PORT=127.0.0.1:8021
ISOL_ENTITY_INDEXDB_INDEX_DOCKER_PORT=127.0.0.1:8022
# address on the Docker host to expose the QMS ports on
ISOL_ENTITY_VECTOR_QMS_ACI_DOCKER_PORT=127.0.0.1:8024
# when ISOL_ENTITY_IDOL_SECURITY_MODE=community, these should be set to the corresponding configuration values used in
# the Community component (SecurityACLFormat, SecurityACLCheck)
ISOL_ENTITY_IDOL_SECURITY_ACL_FORMAT=<E=B!>:U:<U=SLE+>:G:<G=SLE+>:NU:<NU=SLE->:NG:<NG=SLE->
ISOL_ENTITY_IDOL_SECURITY_ACL_CHECK=NU=[DU]?F:-,NG=[DG]?F:-,E=1?P:-,U=[DU]?P:-,G=[DG]?P:F
